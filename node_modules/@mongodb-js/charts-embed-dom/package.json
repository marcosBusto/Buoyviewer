{
  "name": "@mongodb-js/charts-embed-dom",
  "version": "2.3.0",
  "description": "JavaScript library for embedding MongoDB Charts",
  "license": "Apache-2.0",
  "main": "dist/charts-embed-dom.cjs.js",
  "module": "dist/charts-embed-dom.esm.js",
  "unpkg": "dist/charts-embed-dom.umd.min.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "docs:check": "ts-node ./bin/lint-src-for-docs.ts",
    "start": "start-storybook -p 3002",
    "test": "jest",
    "test:ci": "yarn test --ci --runInBand --silent --detectOpenHandles",
    "build:umd": "webpack --mode production",
    "release:npm": "yarn docs && ./bin/publish-to-npm-if-version-changed"
  },
  "repository": "https://github.com/mongodb-js/charts-embed-sdk",
  "devDependencies": {
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@leafygreen-ui/palette": "^3.4.2",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/html": "^5.2.5",
    "@storybook/theming": "^5.2.5",
    "@types/chai": "^4.2.4",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.157",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "csstype": "^2.6.7",
    "jest": "^26.6.3",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.0-3",
    "typescript": "^4.1.5",
    "webpack": "^4.35.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "@looker/chatty": "^2.2.0",
    "bson": "4.5.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19"
  }
}
